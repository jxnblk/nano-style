{
  "name": "nano-style",
  "version": "2.0.0-0",
  "description": "Functional css-in-js for react",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src -d dist",
    "start": "ok docs -op 9000",
    "build": "ok build docs",
    "size": "npm run prepublish && bundlesize",
    "cover": "nyc report --reporter=html --reporter=lcov > coverage.lcov",
    "codecov": "codecov",
    "test": "nyc ava"
  },
  "keywords": [],
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "browser-env": "^3.2.5",
    "html-tags": "^2.0.0",
    "ok-cli": "^2.0.8",
    "prop-types": "^15.6.2",
    "sdbm": "^1.0.0",
    "shortid": "^2.2.12"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bundlesize": "^0.17.0",
    "chroma-js": "^1.3.7",
    "codecov": "^3.0.2",
    "nyc": "^12.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "repng": "^3.0.0",
    "sinon": "^6.0.0",
    "styled-system": "^2.3.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "3.2 kb"
    }
  ],
  "ok": {
    "title": "nano-style",
    "basename": "/nano-style"
  }
}
